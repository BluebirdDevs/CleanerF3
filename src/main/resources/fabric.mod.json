{
  "schemaVersion": 1,
  "id": "ntbt",
  "version": "${version}",
  "name": "No Targeted Block Tags",
  "description": "Removes block tags from targeted blocks",
  "authors": ["BluebirdDevs"],
  "contact": {
    "sources": "https://github.com/BluebirdDevs/NoTargetedBlockTags",
    "homepage": "https://github.com/BluebirdDevs/NoTargetedBlockTags",
    "issues": "https://github.com/BluebirdDevs/NoTargetedBlockTags/issues"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/ntbt/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "bluebird.notargetedblocktags.client.NotargetedblocktagsClient"
    ],
    "modmenu": [
      "bluebird.notargetedblocktags.client.ConfigModMenu"
    ]
  },
  "mixins": [
    {
      "config": "notargetedblocktags.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
