{
  "schemaVersion": 1,
  "id": "cleanerdebug",
  "version": "${version}",
  "name": "Cleaner F3",
  "description": "Allows you to edit more F3 settings such as targeted block tags",
  "authors": ["BluebirdDevs"],
  "contact": {
    "sources": "https://github.com/BluebirdDevs/CleanerF3",
    "homepage": "https://github.com/BluebirdDevs/CleanerF3",
    "issues": "https://github.com/BluebirdDevs/CleanerF3/issues"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/cleanerdebug/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "bluebird.cleanerdebug.client.CleanerDebugClient"
    ],
    "modmenu": [
      "bluebird.cleanerdebug.client.ConfigModMenu"
    ]
  },
  "mixins": [
    {
      "config": "cleanerdebug.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": ">=${minecraft_version}"
  }
}
